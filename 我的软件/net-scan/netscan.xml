      <item>
        <checked>false</checked>
        <name>本地主机地址</name>
        <lang>0</lang>
        <script>'Get all host NICs information
Set NICs = GetObject("winmgmts:").InstancesOf("Win32_NetworkAdapterConfiguration")

BestAddr = ""
BestMatch = 0

'Go through the NICs
For Each NIC in NICs
  if NIC.IPEnabled then
    Match = 0

    'Compare target and host address
    CurrOctets = Split(Input.Current, ".")
    HostOctets = Split(NIC.IPAddress(0), ".")
    For i = LBound(CurrOctets) to UBound(CurrOctets)
      if CurrOctets(i) = HostOctets(i) then
        Match = Match + 1
      End if
    Next
    if BestMatch &lt; Match then
      BestMatch = Match
      BestAddr = NIC.IPAddress(0)
    End if
  End if
Next

'Print out host address
Output.Write BestAddr</script>
